{
    "liveServer.settings.port": 5502,
    "editor.renderWhitespace": "trailing",
    "editor.cursorStyle": "underline",
    "workbench.colorCustomizations": {},
    "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
    "workbench.preferredHighContrastLightColorTheme": "Kimbie Dark",
    "workbench.iconTheme": "vscode-icons",
    "workbench.preferredLightColorTheme": "GitHub Dark Dimmed",
    "scss.lint.duplicateProperties": "warning",
    "scss.lint.float": "warning",
    "liveSassCompile.settings.autoprefix": [],
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/public/css"
        }
    ],
    "prettier.bracketSameLine": true,
    "prettier.tabWidth": 4,
    "prettier.quoteProps": "consistent",
    "liveServer.settings.donotVerifyTags": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "scss.hover.references": false,
    "editor.hover.sticky": false,
    "editor.hover.delay": 700,
    "liveServer.settings.donotShowInfoMsg": true,
    "[html]": {
        "editor.defaultFormatter": "svipas.prettier-plus"
    },
    "vscord.status.idle.timeout": 10000000000,
    "vscord.status.details.text.idle": "Looking at my Five to see what broke",
    "vscord.status.details.text.notInFile": "Not Coding",
    "liveSassCompile.settings.watchOnLaunch": true,
    "liveSassCompile.settings.useNewCompiler": true,
    "editor.minimap.enabled": false,
    "glassit.alpha": 236,
    "window.zoomLevel": -1,
    "workbench.colorTheme": "Monokai Pirokai - Beach Sunset",
    "editor.fontFamily": "'Fira Code Nerd Font', 'Jetbrains Mono Nerd Font', 'Hack Nerd Font', 'monospace'",
    "editor.fontLigatures": true,
    "editor.tabSize": 4,
    "fiveServer.navigate": false,
    "terminal.integrated.fontFamily": "'Fira Code Nerd Font', 'Jetbrains Mono Nerd Font', 'Hack Nerd Font', 'monospace'"
}
